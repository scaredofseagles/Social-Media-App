{
  "name": "social-media-app",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "migrate:make": "knex migrate:make migrations",
    "migrate": "knex migrate:latest",
    "unmigrate": "knex migrate:rollback",
    "migrate:down": "knex migrate:down",
    "mysql": "/usr/local/mysql/bin/mysql -u nodeuser -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scaredofseagles/Social-Media-App.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^1.18.7",
    "knex": "^0.21.17",
    "mysql": "^2.18.1"
  },
  "bugs": {
    "url": "https://github.com/scaredofseagles/Social-Media-App/issues"
  },
  "homepage": "https://github.com/scaredofseagles/Social-Media-App#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "styled-components": "^5.2.1"
  }
}
